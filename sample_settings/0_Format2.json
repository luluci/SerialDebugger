							"name": "Rx_Pattern_3",
							"active": true,
							"matches": [
								{ "type": "any" },
								{ "value": 2 },
								{ "value": 3 },
								{ "type": "any" },
								{ "value": 3 }
							]
						},
						{
							"name": "Rx_Pattern_*",
							"matches": [
								{ "type": "script", "script": "func" },
								{ "type": "timeout", "msec": 100 },
								{ "type": "any" },
								{ "type": "value", "value": 1 },
								{ "value": 2 },
								{ "type": "any" },
								{ "type": "any" },
								{ "type": "any" }
							]
						}
					]
				}
			]
